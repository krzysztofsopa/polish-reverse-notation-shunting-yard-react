{
  "name": "validator",
  "version": "1.0.0",
  "description": "validator es6 library",
  "main": "src/index.js",
  "scripts": {
    "start": "budo src/Validator.js:build.js --live --dir static -t babelify | garnish",
    "prod": "browserify src/Validator.js --t babelify | uglifyjs -nm > build/validator.js",
    "test": "node node_modules/.bin/mocha --compilers js:babel/register"
  },
  "keywords": [
    "es6"
  ],
  "author": "Krzysztof Sopa <krzysztof.sopa@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.1.11",
    "babelify": "^6.0.2",
    "browserify": "^9.0.8",
    "budo": "^3.1.1",
    "garnish": "^2.1.3",
    "uglify-js": "^2.4.20",
    "watchify": "^3.1.2"
  },
  "dependencies": {
    "domready": "^1.0.7",
    "mocha": "^2.4.5"
  }
}
